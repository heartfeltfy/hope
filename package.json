    "@tailwindcss/vite": "^4.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.510.0",
    "mobx": "^6.13.7",
    "mobx-persist-store": "^1.1.8",
    "mobx-react": "^9.2.0",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.2.0",
